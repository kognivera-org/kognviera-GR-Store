    "vm": "^0.1.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "ajv": "^7.2.4",
    "babel-loader": "^7.1.5",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^7.1.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "file-loader": "^6.2.0",
    "node-sass": "^9.0.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.0.2",
    "process": "^0.11.10",
    "sass-loader": "^14.1.1",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "7.1.1",

{
  "name": "LiverpoolGiftRegistryInStore",
  "description": "a fullstack react webapp with redux, hmr, hapi, sequelize",
  "author": "EunseomEom <me@eseom.org> (http://github.com/eseom)",
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eseom/react-hapi-fullstack-boilerplate"
  },
  "homepage": "https://github.com/eseom/react-hapi-fullstack-boilerplate",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "hapi",
    "sequelize",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "es2015": true
        }
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "main": "bin/server.js",
  "scripts": {
    "start": "npm-run-all server",
    "server": "better-npm-run server",
    "exec": "better-npm-run exec",
    "build": "better-npm-run build",
    "preinstall": "node package.preinstall.js",
    "postinstall": "yarn build",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "better-npm-run dev:server",
    "dev:packager": "better-npm-run dev:packager",
    "test": "karma start",
    "test:node": "better-npm-run test:node",
    "test:node:watch": "better-npm-run test:node:watch"
  },
  "betterScripts": {
    "server": {
      "command": "node bin/server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "exec": {
      "command": "node bin/console",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev:server": {
      "command": "nodemon bin/server --mode development",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "dev:packager": {
      "command": "node bin/packager --mode development",
      "env": {
        "NODE_PATH": "./src"
      }
    },
    "build": {
      "command": "webpack --mode production --config ./tools/webpack.production.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-bundle": {
      "command": "webpack --verbose --colors --display-error-details --config ./tools/webpack.config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test:node": {
      "command": "mocha $(find src/server -name '*-test.js') --compilers js:./tools/babel-require",
      "env": {
        "NODE_ENV": "TEST"
      }
    },
    "test:node:watch": {
      "command": "mocha $(find src/server -name '*-test.js') --compilers js:./tools/babel-require --watch",
      "env": {
        "NODE_ENV": "TEST"
      }
    }
  },
  "dependencies": {
    "@hapi/hapi": "^21.3.9",
    "@hapi/hoek": "^11.0.4",
    "@hapi/inert": "^7.1.0",
    "@hapi/nes": "^13.0.1",
    "@hapi/vision": "^7.0.3",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "babel-core": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "base-64": "^1.0.0",
    "bcrypt-nodejs": "^0.0.3",
    "better-npm-run": "^0.0.14",
    "crypto-browserify": "^3.12.0",
    "fs": "0.0.1-security",
    "hapi-nested-route": "^0.1.1",
    "hapi-swagger": "^17.2.1",
    "html-pdf": "^3.0.1",
    "html2canvas": "^1.0.0-alpha.9",
    "https-browserify": "^1.0.0",
    "ip": "^1.1.5",
    "joi": "^17.12.3",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.5",
    "memjs": "^1.2.0",
    "mini-css-extract-plugin": "^2.8.1",
    "moment": "^2.30.1",
    "nes": "^10.0.2",
    "node-cache": "^4.1.1",
    "node-gyp": "^10.1.0",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "os": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "pretty-error": "^2.0.2",
    "prop-types": "^15.6.0",
    "react": "^17.0.2",
    "react-barcode": "^1.3.2",
    "react-code-input": "^3.10.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^5.2.0",
    "react-onclickoutside": "^6.7.1",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-step-progress-bar": "^1.0.3",
    "react-transition-group": "^2.2.1",
    "redux": "^3.6.0",
